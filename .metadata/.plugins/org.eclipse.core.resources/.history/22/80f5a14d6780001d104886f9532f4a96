<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page session="true" %>
<!-- 해당 페이지가 HttpSessio을 사용할지 여부를 지정하는 속성 -->
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>page 디렉티브 연습 - session 속성 </title>
</head>
<body>
<h2>page 디렉티브 연습 - session 속성 </h2>
<%="이 페이지는 세션이 유지되는 페이지 입니다." %>
</body>
</html>